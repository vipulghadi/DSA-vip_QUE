------------------------0(n)-----------------------------------------------------------------------
void frequencyCount(vector<int>& a,int n, int p)
    { 
       p=p+1;
        for(int i=0;i<n;i++){
            if((a[i]%p)<=n)
                a[(a[i]-1)%p]+=p;
        }
        for(int i=0;i<n;i++){
            a[i]/=p;
        }

    }
    -------------------------------------------------------------------------------------------
